{
  "name": "reference-project",
  "version": "1.0.0",
  "description": "Sample TS reference project",
  "scripts": {
    "cache-clean": "npm cache clean --force",
    "install-dependency": "npm install",
    "webdriver-start": "npx webdriver-manager update && npx webdriver-manager start --seleniumPort 4444",
    "webdriver-stop": "npx webdriver-manager shutdown",
    "compile": "npx eslint . --ext .ts --fix && tsc",
    "generate-report": "allure generate --clean allure-results",
    "generate-report-open": "allure generate --clean allure-results && allure open",
    "eslint-fix": "npx eslint . --ext .ts --fix",
    "test-local": "jest ui/specs/sanity/app/*.test.ts --maxWorkers=2 --forceExit",
    "test-demo": "jest ui/specs/sanity/app/demo.test.ts",
    "test-api": "jest webservices/specs/**/**/**/*.test.ts --group=$Group --maxWorkers=5 --forceExit",
    "test-ui": "jest ui/specs/**/web/*.test.ts --group=$Group --maxWorkers=5 --forceExit",
    "test-app": "jest ui/specs/**/app/*.test.ts --group=$Group --maxWorkers=5 --forceExit",
    "prepare-husky": "husky install"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/chai": "^4.2.21",
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^27.5.0",
    "@types/jsonpath-plus": "^5.0.2",
    "@types/jsonschema": "^1.1.1",
    "@types/node": "^17.0.32",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@wdio/allure-reporter": "^7.9.1",
    "@wdio/browserstack-service": "^7.32.1",
    "allure-commandline": "^2.13.8",
    "commitlint": "^16.2.4",
    "del": "^6.0.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-jasmine2": "^28.1.0",
    "jest-puppeteer-allure": "^1.4.0",
    "jest-runner": "^28.1.0",
    "jest-runner-groups": "^2.1.0",
    "latest": "^0.2.0",
    "moment": "2.29.4",
    "sync-request": "^6.1.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4",
    "wdio-lambdatest-service": "^2.0.3"
  },
